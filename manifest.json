{
    "manifest_version": 2,
    "name": "Sound Control",
    "version": "0.0.0",

    "default_locale": "en",
    "description": "Mutes all other tabs",
    "icons": {
        "128": "icon128.png"
    },

    "author": "Andrew Zaplitnyak",
    "background": {
         "persistent": false,
         "scripts": ["background.js"]
    },
    "permissions": [
        "alarms",
        "tabs",
        "webNavigation"
    ]
}

